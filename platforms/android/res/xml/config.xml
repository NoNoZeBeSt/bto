<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.nydus.bto" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Accelerometer">
        <param name="android-package" value="org.apache.cordova.devicemotion.AccelListener" />
    </feature>
    <feature name="Vibration">
        <param name="android-package" value="org.apache.cordova.vibration.Vibration" />
    </feature>
    <feature name="Compass">
        <param name="android-package" value="org.apache.cordova.deviceorientation.CompassListener" />
    </feature>
    <allow-intent href="market:*" />
    <icon density="ldpi" src="icons/android/ldpi.png" />
    <icon density="mdpi" src="icons/android/mdpi.png" />
    <icon density="hdpi" src="icons/android/hdpi.png" />
    <icon density="xhdpi" src="icons/android/xhdpi.png" />
    <name>Bouffeteo</name>
    <description>
        Where do we eat ? I don't know, but I'm hungry !!
    </description>
    <author email="nonozebest@gmail.com" href="http://i.liketomove.it">
        DotDotDot
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
